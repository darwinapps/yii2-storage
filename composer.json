{
    "name": "darwinapps/yii2-storage",
    "description": "The Storage extension for the Yii framework",
    "keywords": ["yii2", "storage", "file", "s3"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "support": {
        "issues": "https://github.com/darwinapps/yii2-storage/issues",
        "source": "https://github.com/darwinapps/yii2-storage"
    },
    "authors": [
        {
            "name": "Aleksandr Guidrevitch",
            "email": "darwinapps@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "symfony/symfony": "2.6.*",
        "google/apiclient": "1.0.*@beta"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "codeception/codeception": "*"
    },
    "autoload": {
        "psr-4": {
            "darwinapps\\storage\\": ""
        }
    }
}
